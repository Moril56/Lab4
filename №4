#include <stdio.h>
#include <math.h>

int main() {
    double a = 1.25; // Початок інтервалу
    double b = 6.75; // Кінець інтервалу
    double dx = 0.25; // Крок
    double x, y;

    // Вивід заголовка таблиці
    printf("x\t\t y\n");
    printf("-------------------\n");

    // Цикл обчислення і виведення значень функції
    for (x = a; x <= b; x += dx) {
        if (x >= 0) {
            y = pow(x, 0.25); // Обчислення значення функції
            printf("%.2f\t|\t %.4f\n", x, y); // Виведення значення x та відповідного y
        } else {
            printf("%.2f\t|\t Невизначено\n", x); // Виведення "Невизначено", якщо аргумент менший за 0
        }
    }

    return 0;
}
