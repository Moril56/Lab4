#include <stdio.h>

int main() {
    int sum = 0;
    
    // Проходимо через усі числа у діапазоні від -20 до 10
    for (int i = -20; i <= 10; i++) {
        // Якщо число додатнє, додаємо його до суми
        if (i > 0) {
            sum += i;
        }
    }
    
    // Обчислюємо квадрат суми
    int square_sum = sum * sum;
    
    // Виводимо результат
    printf("Квадрат суми всіх додатніх чисел у діапазоні [-20;10] дорівнює %d\n", square_sum);
    
    return 0;
}
